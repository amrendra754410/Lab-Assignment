package Class_Work;
import java.util.*;
public class Question_07 {
	public static void main(String args []) {
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter n: ");
		int n=sc.nextInt();
		for(int i=1;i<=n;i++) {
			for(int j=0;j<i;j++) {
				System.out.print((char)(65+j));
			}
			System.out.println();
		}
	}

}
